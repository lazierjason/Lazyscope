<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
	
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8" />
	<title>
		Content viewer
	</title>
	<meta name="Nationality" content="English" />

	<style type="text/css" media="all">
		@import 'stylesheets/notifier.css';
	</style>
	<script type="text/javascript" src="javascripts/util.js"></script>
	<script type="text/javascript" src="javascripts/notifier.js"></script>
</head>

<body onload="Notifier.init();">
	<div id="container">
		<div class="main" id="main" onclick="Notifier.switchToMainWin()"></div>
		<div class="header">
			<div class="title"><span id="update-count">0 update</span> from All subscriptions</div>
			<div class="controls">
				<a class="button-prev" onclick="Notifier.prev()">prev</a>
				<span class="paging"><span id="current-page">0</span> of <span id="total-page">0</span></span>
				<a class="button-next" onclick="Notifier.next()">next</a>
			</div>
		</div>
	</div>
</body>

<script type="text/javascript">
window.addEventListener('load', function(e) {
	Notifier.init();
});
</script>

</html>
